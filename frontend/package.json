{
  "name": "electrotempo-front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "cypress-local-test": "npx cypress run --env host=http://localhost:3000",
    "cypress-dev-test": "npx cypress run --env host=https://development.electrotempo.net",
    "cypress-stage-test": "npx cypress run --env host=https://staging.electrotempo.net",
    "cypress-prod-test": "npx cypress run --env host=https://electrotempo.net",
    "unit-test": "react-scripts test --passWithNoTests",
    "e2e-test": "npx playwright test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "format-check": "prettier --check src/**/*.{js,jsx,ts,tsx}",
    "lint": "tsc --noEmit && eslint --max-warnings=0 src/**/*.{js,jsx,ts,tsx}"
  },
