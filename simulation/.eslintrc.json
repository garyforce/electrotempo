{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        // Indent with 4 spaces
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-undef": "error",
        "no-console": "off"
    }
}